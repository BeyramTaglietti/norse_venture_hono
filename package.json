    "google-auth-library": "^10.5.0",
    "hono": "^4.10.7",
    "jose": "^6.1.3",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "sharp": "^0.34.5",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/bun": "latest",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pg": "^8.15.6",
    "typescript-eslint": "^8.48.0",
    "drizzle-kit": "^0.31.7",
    "eslint": "^9.39.1",
