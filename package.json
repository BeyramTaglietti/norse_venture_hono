{
  "name": "norse_venture_hono",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "dev": "NODE_ENV=development bun run --hot src/index.ts",
    "build": "bun build --entrypoints ./src/index.ts --outdir ./build --target bun",
    "start": "NODE_ENV=production bun build/index.js",
    "db:generate": "drizzle-kit generate:pg",
    "db:push": "drizzle-kit push:pg",
    "db:migrate": "bun run src/drizzle/migrate.ts",
    "lint": "eslint .",
    "prepare": "husky"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.540.0",
    "@aws-sdk/s3-request-presigner": "^3.540.0",
    "@hono/zod-validator": "^0.2.1",
    "drizzle-orm": "^0.30.6",
    "google-auth-library": "^9.7.0",
    "hono": "^4.1.5",
    "pg": "^8.11.4",
    "sharp": "^0.33.3",
    "verify-apple-id-token": "^3.1.2"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/pg": "^8.11.4",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-drizzle": "^0.2.3",
    "husky": "^9.0.11",
    "prettier": "^3.2.5"
  }
}
